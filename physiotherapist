server {
    listen 80;
    server_name your_domain.com www.your_domain.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    
    location /static/ {
        root /var/www/physiotherapist_webpage;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/var/www/physiotherapist_webpage/physiotherapist.sock;
    }
} 